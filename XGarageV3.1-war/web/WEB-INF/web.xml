<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>Reportes</servlet-name>
        <servlet-class>como.garage.conexion.Reportes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RespuestaParqueadero</servlet-name>
        <servlet-class>como.garage.conexion.RespuestaParqueadero</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RespuestaUsuario</servlet-name>
        <servlet-class>como.garage.conexion.RespuestaUsuario</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SolicitudIngreso</servlet-name>
        <servlet-class>como.garage.conexion.SolicitudIngreso</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>Reportes</servlet-name>
        <url-pattern>/Reportes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RespuestaParqueadero</servlet-name>
        <url-pattern>/RespuestaParqueadero</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RespuestaUsuario</servlet-name>
        <url-pattern>/RespuestaUsuario</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SolicitudIngreso</servlet-name>
        <url-pattern>/SolicitudIngreso</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
